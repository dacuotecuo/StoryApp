  color: #999;
  font-size: 24rpx;
  text-decoration: line-through;
}

.group_btn {
  background-color: #f3882f;
  border-radius: 50px;
  color: #fff;
  font-size: 26rpx;
  height: 50rpx;
  line-height: 1;
  display: -webkit-box;
  -webkit-box-align: center;
  -webkit-box-pack: center;
  overflow: hidden;
  padding: 0 2rpx;
  width: 170rpx;
  position: absolute;
  right: 0;
  bottom:0;
  top:0;
  margin: auto;
  -webkit-animation: flashing 1s 0.2s reverse infinite linear;
  animation: flashing 1s 0.2s reverse infinite linear;
}
@-webkit-keyframes flashing {
  0%{
    -webkit-transform: scale(0.95);
  }
  50%{
    -webkit-transform: scale(1);
  }
  100%{
    -webkit-transform: scale(0.95);
  }
}
@keyframes flashing {
  0%{
    transform: scale(0.95);
  }
  50%{
    transform: scale(1);
  }
  100%{
    transform: scale(0.95);
  }
}
.group_btn.group_btn_hover:active{background:#ef7f21;}
.group_btn:after {
  border: none;
}
.group_btn_wrap.incomplete .group_btn_txt {
  color: #656565;
}

.group_btn_wrap.incomplete .group_btn {
