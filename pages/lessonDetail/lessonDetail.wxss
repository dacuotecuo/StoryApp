.page_wrap{
    box-sizing:border-box;
    height: 100%;
    background: #f8f8f8;
    padding:0 0 51px;
    position:relative;
}
/* .page_wrap.inset_height .footer{box-sizing:border-box;padding-bottom:34px!important;}  */
/* .page_wrap.inset_height{padding-top:34px!important;} */
/* .page_wrap.inset_height .footer{box-sizing:border-box;padding-bottom:34px!important;}  */
.page_wrap.inset_height{padding-bottom:85px!important;}
.page_wrap.inset_height .footer{box-sizing:border-box;padding-bottom:34px!important;}
.page_con{height:100%;}
.act_img{
    display: block;
    width: 100%;
    height: 320rpx;
}
.act_info{
    height: 89px;
    padding: 0 30rpx 30rpx;
    background: #fff;
}
.act_info .fl{
    float: left;
    font-size: 0;
    margin-top: 15px;
}
.act_info .time{
    display: inline-block;
    height: 20px;
    padding: 0 8px;
    line-height: 20px;
    font-size: 15px;
    color: #fff;
    background: #F05143;
}
.act_info .group_info{
    font-size:14px;
    color: #111;
    opacity: .6;
    margin-top: 10px;
}
.act_info .group_info .group_sum{margin-left: 10px;}
.act_info .left_num{
    opacity: 0.4;
    font-size: 12px;
    color: #111;
    margin-top: 5px;
}

.act_info .price{
    float: right;
    text-align: center;
    margin-top: 16px;
}
.act_info .ori_price{
    color: #F05143;font-size:18px;
    text-decoration:line-through;line-height: 1;
}
.act_info .now_price{
    font-family: PingFangSC-Medium;
    font-size: 27px;
    color: #F05143;
    line-height: 1;
    margin-top: 5px;
}
.box{
    min-height: 150px;
    text-align: center;
    background: #fff;
    padding: 15px 30rpx;
    overflow: hidden;
    box-sizing: border-box
}
.box .left_time{
    display: block;
    font-size: 16px;
    color: #111;
}
.box .partner{display:flex;margin:15px 0;justify-content:center;}
.box .partner .tag{
    position: absolute;
    left: 50%;
    bottom:0px;
    color: #fff;
    width: 29px;
    height: 14px;
    line-height: 14px;
    text-align: center;
    font-size: 10px;
    border-radius: 50px;
    background: #F05143;
    -webkit-transform:translateX(-50%);
}
.box .partner_avatar{position:relative;}
.box image{
    display: block;
    width: 32px;
    height: 32px;
    border-radius: 50%;
    margin:0 5px;
}

.box .variable_btn{
    position: relative;
    display:inline-block;
    padding:0;
    width: 270px;
    height: 34px;
    margin: 10px auto 0;
    line-height: 34px;
    background: #F3882F;
    border-radius: 50px;
    font-size:15px;
    color: #fff;
    text-align: center;
    z-index: 100;
    box-shadow:0 0 5px rgba(243,136,47,.5);
}
.box .variable_btn:active{opacity:.7;}
.module{
    position: relative;
    background: #fff;
    margin-top: 10px;
    padding: 30rpx
}
.module .title{
    font-family: PingFangSC-Medium;
    font-size: 16px;
    color: #111;
    display: block;
    font-weight: bold;
}

.fivestar .p{
    font-size: 14px;
    color: #111;
    opacity: .6;
    overflow: hidden;
}
.fivestar .p:first-of-type{margin-top: 10px;}

.fivestar_head{display:flex;justify-content:space-between;}
.fivestar_item{}
.fivestar_more{color:#F3882F;font-size:16px;margin-right:15px;}
.rule{background: #fff;margin-top: 10px;padding: 30rpx 0;}
.rule .title{
    padding-left: 30rpx;
    font-family: PingFangSC-Medium;
    font-size: 16px;
    color: #111;
    line-height: 1;display: block;
    font-weight: bold
}
.rule image{width: 100%;}
.rules{
    padding: 0 30rpx;
    font-size: 14px;
    color: #111;
}
.rules view:nth-child(n+2){margin-top: 5px;}
.rule .idx{float: left;display: block}
.rule .txt{overflow: hidden;display: block}
.rule_box{display:block;margin:10px 0;}
.rule_link{border-bottom:1px solid #F3882F;color:#F3882F;display:inline-block;font-size:16px;}
.rule_link:active{opacity:.7;}
.teach_head{margin-top: 10px;}
.hongbao{padding-right: 0;padding-left: 0;}
.hongbao .title{padding-left: 30rpx}
.hongbao .num{font-size: 12px;color: #111;opacity: .4;}
.hongbao .items{margin-top: 10px;}
.hongbao .item{
    position: relative;
    /* display: flex;
    align-items: center; */
    height: 45px;
    line-height: 45px;
    font-size: 26rpx;
    padding-right: 30rpx;
    padding-left: 30rpx;
}
.hongbao .item:after{
    position: absolute;
    content:'';
    bottom: 0;
    display: block;
    left: 30rpx;right: 0;
    border-bottom: 1px solid #E5E5E5;
}
.hongbao .avatar{
    width: 30px;
    height: 30px;
    border-radius: 50%;
    margin-right: 10px;
    vertical-align: -10px;
}

.hongbao .item text{vertical-align: middle}
.hongbao .item .uname{display: inline-block;width: 260rpx;text-overflow: ellipsis;overflow: hidden;white-space: nowrap;vertical-align: top}
.hongbao .item .from{float: right;opacity: .6;}
.hongbao .item .time{opacity: .6;display: inline-block;width: 150rpx;text-align: center;vertical-align: top}
.kf{
    position: fixed;
    width: 60px;
    height: 60px;
    top: 45%;
    right:10rpx;
    z-index: 100;
}
.kf:active{opacity:.7;}
.kf image{width: 100%;height: 100%;}
.stamp{position: relative;}
.stamp .mark{
    position: absolute;
    width: 308rpx;
    height: 315rpx;
    z-index: 99;
    top: 45px;
    right: 0;
}
.footer{
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: 99;
    text-align: center;
    background: #fff;
    height:104rpx;
}
.footer .toIndex{
    background: #fff;
    color: #F3882F;
    float: left;
    width: 50%;
    cursor: pointer;
}
.footer .toIndex .og_price{
  font-size:28rpx;
  line-height: 50rpx;
  color:#111;
  opacity: .4;
}
.footer .isPaid{
  height: 104rpx;
  line-height: 104rpx;
  text-align: center;
  background: #F3882F;
  color: #fff;
  font-size:34rpx;
}
.footer .isPaid button{margin: 0;padding: 0;color:inherit;background: inherit;line-height: inherit;border-radius: inherit;border: none;font-size: inherit;}
.footer .isPaid button:after{border: none;}
.footer .toIndex:active{opacity:.7;}
.footer .fr{overflow: hidden;}
.footer .tips{
    height:104rpx;
    text-align: center;
    background: #F3882F;
    color: #fff;
    font-size: 34rpx;
    line-height: 104rpx;
}
.footer .tips:active{opacity:.7;}
/* .footer_box{
    height: 50px;
    font-size: 17px;
    line-height: 50px;
} */
.fivestar{
    padding: 20rpx 0 0;
}
.fivestar .title{
    padding: 0 30rpx 20rpx;
}
.fivestar image{
    width: 100%;
}
.mask{
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0,0,0,.5);
    z-index:200;
}
.mask .customBox{
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    width: 280px;
    background: #fff;
    box-sizing: border-box;
    padding: 34px 15px 20px;
    border-radius: 12px;
    text-align: center;

}
.mask .customBox text{
    font-size: 17px;
    color: #111;
    line-height: 24px;
    display:table;
    margin:0 auto;
    text-align:justify;
}
.mask .customBox .goGroupList{
    height: 40px;
    line-height: 40px;
    text-align: center;
    margin-top: 36px;
    background-image: linear-gradient(45deg, #FC7143 0%, #FC9043 100%);
    border-radius: 50px;
    color: #fff;
}
.mask .customBox .goGroupList:active{opacity:.7;}
.mask .close{
    position: absolute;
    width: 30px;
    height: 30px;
    top: -45px;
    right: 0;
}
.mask .close image{
    width: 100%;
    height: 100%;
}
.box .secBtn{
    border: 1px solid #F3882F;
    background: #fff;
    font-size: 15px;
    color: #F3882F;
}
swiper{height: 160px;}

.pop_mask {
  background-color: rgba(0, 0, 0, 0.2);
  bottom: 0;
  left: 0;
  position: fixed;
  right: 0;
  top: 0;
  z-index: 1000;
}

.pop_share {
  background-color: #fff;
  position: fixed;
  height: 360px;
  width: 300px;
  left: 50%;
  top: 50%;
  margin-top: -180px;
  margin-left: -150px;
  color: #fff;
  border-radius: 16px;
  text-align: center;
  padding: 15px 25px;
}
.pop_close {
  display: block;
  height: 30px;
  position: absolute;
  top: -32px;
  right: 8px;
  width: 30px;
}
.pop_img,.pop_img1,.pop_img2 {
  display: block;
  width: 100%;
}
.pop_img1{
  width:190px;
  height: 63px;
  margin: 0 auto;
}
.pop_img2{
  width:190px;
  height: 104px;
  margin: 0 auto;
}
.pop_1,.pop_2 {
  color: #111;
  font-size: 17px;
  font-weight: bold;
  height: 30px;
  line-height: 20px;
}
.pop_3{
  margin:15px auto;
}
.pop_btn{
  font-size:34rpx;
  width:100%
}
.pop_4 {
  height: 40px;
  line-height: 40px;
  margin: 0 auto;
  background-image: linear-gradient(45deg, #FC7143 0%, #FC9043 100%);
  box-shadow: 0 4px 12px 0 rgba(240, 128, 33, 0.40);
  border-radius: 100px;
  margin-top: 30px;
  color: #fff;
}
.s_c{
vertical-align: middle;
}
.wxinfo{
  opacity: 0.4;
font-size: 14px;
color: #111111;
