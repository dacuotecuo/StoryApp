.header{position: relative;}
.page_wrap{position: relative;}
.success_scroll{height:48rpx;position: absolute;left:30rpx;bottom:11rpx;padding:0 20rpx 0 0;width:520rpx;background:rgba(0,0,0,0.3);border-radius: 100px;z-index: 100;}
.success_scroll swiper-item {display: -webkit-box;height: 48rpx;-webkit-box-align: center;white-space: nowrap;}
.success_scroll .pt_item{height:48rpx;}
.success_scroll .avatar_img{width:50rpx;height: 50rpx;border-radius: 50%;overflow: hidden;}
.success_scroll .avatar_name{color:#EF8021;}
.success_scroll .scroll_txt{font-size: 28rpx;color:#fff;line-height: 1;white-space: nowrap;text-overflow: ellipsis;overflow: hidden;width: 89%;}
.pt_wrap{margin-top:10rpx;background: #fff;}
.pt_wrap swiper-item{height:120rpx;}
.pt_wrap .pt_title{height:82rpx;display: -webkit-box;-webkit-box-align: center;color:#111;font:bold 32rpx/1 'PingFangSC-Medium';padding-left: 30rpx;}
.pt_wrap .pt_cnt .pt_list{position:relative;display: -webkit-box;height: 120rpx;-webkit-box-align: center;-webkit-box-pack: justify;padding: 0 30rpx;}
.pt_wrap .pt_cnt .pt_list:after{content:'';position: absolute;left: 30rpx;right: 0;bottom: 0;height: 1px;background:#e6e6e6;}
.pt_wrap swiper{height: auto}
.pt_cnt .pt_list:last-child:after{display: none;}
.pt_cnt .pt_list image{display: block;width: 80rpx;height: 80rpx;border-radius: 50%;overflow: hidden;}
.pt_cnt .pt_list .name{display:block;font-size:32rpx;color:#111;line-height: 1;width: 35%;white-space: nowrap;text-overflow: ellipsis;overflow: hidden;text-align: center;}
.pt_cnt .pt_list .sum_txt{font-size:28rpx;line-height: 1;width: 30%;}
.pt_cnt .pt_list .sum_txt view:first-child{line-height: 34rpx;}
.pt_cnt .pt_list .sum_txt .c_orange{color:#F3882F;}
.pt_cnt .pt_list .sum_txt .surplus_date{font-size: 24rpx;color:#676767;line-height: 1;margin-top:4rpx;}
.pt_cnt .pt_list .pt_btn{width:110rpx;height: 50rpx;border-radius: 100px;background:#F3882F;display: -webkit-box;-webkit-box-align: center;-webkit-box-pack: center;font-size:26rpx;color:#fff;padding: 0;}
.pt_cnt .pt_list .pt_btn button{padding: 0;border: none;background: none;font-size:26rpx;line-height: 1;color:#fff;overflow: inherit;}
.pt_cnt .pt_list .pt_btn button:after{border: none;}
